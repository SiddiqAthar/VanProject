<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">

            <stroke android:width="1dip" android:color="@color/button_pressed" />
            <!--<gradient android:angle="-90" android:endColor="#689a92" android:startColor="#345953" />-->
            <corners android:radius="5dip" />
            <solid android:color="@color/button_pressed" />
        </shape>
    </item>

    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="@color/button_pressed" />
            <corners android:radius="5dip" />
            <solid android:color="@color/button_pressed" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dip" android:color="@color/button_default_green" />
            <corners android:radius="5dip" />
            <!--   <gradient android:angle="-90" android:endColor="#58857e" android:startColor="#8dbab3" />
           -->
            <solid android:color="@color/button_default_green" />
        </shape>
    </item>
</selector>
